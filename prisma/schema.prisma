datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "debian-openssl-1.1.x"]
}

model Order {
  id         String   @id @default(cuid())
  customer   String
  status     String
  total      Float
  createdAt  DateTime @default(now())
  updatedAt  DateTime @updatedAt
}
