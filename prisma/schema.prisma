generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["driverAdapters"]
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Order {
  id        String   @id @default(cuid())
  customer  String
  status    String
  total     Float
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
